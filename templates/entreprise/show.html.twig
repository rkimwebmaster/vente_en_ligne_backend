{% extends 'base.html.twig' %}

{% block title %}Entreprise{% endblock %}

{% block body %}
    <h1>Entreprise</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entreprise.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entreprise.nom }}</td>
            </tr>
            <tr>
                <th>Idnat</th>
                <td>{{ entreprise.idnat }}</td>
            </tr>
            <tr>
                <th>Rccm</th>
                <td>{{ entreprise.rccm }}</td>
            </tr>
            <tr>
                <th>Sigle</th>
                <td>{{ entreprise.sigle }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ entreprise.logo }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
