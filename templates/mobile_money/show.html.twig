{% extends 'base.html.twig' %}

{% block title %}MobileMoney{% endblock %}

{% block body %}
    <h1>MobileMoney</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mobile_money.id }}</td>
            </tr>
            <tr>
                <th>Designation</th>
                <td>{{ mobile_money.designation }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ mobile_money.description }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ mobile_money.createdAt ? mobile_money.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ mobile_money.updatedAt ? mobile_money.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_mobile_money_index') }}">back to list</a>

    <a href="{{ path('app_mobile_money_edit', {'id': mobile_money.id}) }}">edit</a>

    {{ include('mobile_money/_delete_form.html.twig') }}
{% endblock %}
